{
  "expo": {
    "name": "A custom smart video player",
    "description": "A custom smart video player with React Native & Expo",
    "scheme": "smartvideoplayer",
    "slug": "smartvideoplayer",
    "privacy": "public",
    "version": "0.0.1",
    "platforms": [
      "android",
      "ios",
      "web"
    ],
    "githubUrl": "https://github.com/dbarros1979/smart-video-player",
    "orientation": "portrait",
    "primaryColor": "#000000",
    "icon": "src/assets/images/icon.png",
    "splash": {
      "backgroundColor": "#000000",
      "image": "src/assets/images/splash.png",
      "resizeMode": "contain"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "versionCode": 2
    },
    "ios": {
      "buildNumber": "0.0.1",
      "supportsTablet": true
    }
  }
}